# Write annotation by starting with '#', which would be ignored by parser

# Subscription name
name: Ralyo - random 

# Author name 
creator: ralyo

# For samll poster displayed in app
small_poster_url: https://raw.githubusercontent.com/mithyer/ralyo-subscription-demo/main/demo/poster_small.png 

# For large poster displayed in app
large_poster_url: https://raw.githubusercontent.com/mithyer/ralyo-subscription-demo/main/demo/poster_large.png

# Write something about this subscription
description: Songs generated by ralyo

# Change version when you added, removed or replaced songs. If version remains same, new parsing won't process.
version: 1.4


# Path without endpoint is as same as this.
# If index's url was https:://domain/path/index, songs' url would be https:://domain/path/songs
# In this case, the songs' url is https://raw.githubusercontent.com/mithyer/ralyo-subscription-demo/main/demo/songs
songs: songs
